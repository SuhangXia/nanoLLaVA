    <visual><geometry><box size="0.006 0.012 0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.006 0.012 0.02"/></geometry></collision>
  </link>
  <joint name="gripper_left" type="prismatic">
    <parent link="link_6"/><child link="finger_left"/>
    <origin xyz="0 0.006 0.01" rpy="0 0 0"/><axis xyz="0 1 0"/>
    <limit lower="0" upper="0.02" effort="5" velocity="0.5"/>
  </joint>
  <link name="finger_right">
    <inertial><mass value="0.01"/><inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/></inertial>
    <visual><geometry><box size="0.006 0.012 0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.006 0.012 0.02"/></geometry></collision>
  </link>
  <joint name="gripper_right" type="prismatic">
    <parent link="link_6"/><child link="finger_right"/>
    <origin xyz="0 -0.006 0.01" rpy="0 0 0"/><axis xyz="0 -1 0"/>
    <limit lower="0" upper="0.02" effort="5" velocity="0.5"/>
  </joint>
</robot>
