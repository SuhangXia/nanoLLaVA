<?xml version="1.0"?>
<!-- Minimal WidowX 250 (Black arm) for PyBullet. 6R arm + 2 prismatic fingers. No external meshes. -->
<robot name="widowx_250">
  <material name="black">
    <color rgba="0.10 0.10 0.10 1.0"/>
  </material>
  <link name="base_link">
    <inertial><mass value="0.01"/><inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/></inertial>
    <visual><geometry><box size="0.06 0.06 0.04"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.06 0.06 0.04"/></geometry></collision>
  </link>
  <link name="link_1">
    <inertial><mass value="0.2"/><inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/></inertial>
    <visual><geometry><box size="0.04 0.04 0.06"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.04 0.04 0.06"/></geometry></collision>
  </link>
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/><child link="link_1"/>
    <origin xyz="0 0 0.02" rpy="0 0 0"/><axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2"/>
  </joint>
  <link name="link_2">
    <inertial><mass value="0.15"/><inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/></inertial>
    <visual><geometry><box size="0.03 0.08 0.03"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.03 0.08 0.03"/></geometry></collision>
  </link>
  <joint name="joint_2" type="revolute">
    <parent link="link_1"/><child link="link_2"/>
    <origin xyz="0 0 0.03" rpy="0 0 0"/><axis xyz="0 1 0"/>
    <limit lower="-2.4" upper="2.4" effort="10" velocity="2"/>
  </joint>
  <link name="link_3">
    <inertial><mass value="0.12"/><inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/></inertial>
    <visual><geometry><box size="0.025 0.07 0.025"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.025 0.07 0.025"/></geometry></collision>
  </link>
  <joint name="joint_3" type="revolute">
    <parent link="link_2"/><child link="link_3"/>
    <origin xyz="0 0.04 0" rpy="0 0 0"/><axis xyz="0 1 0"/>
    <limit lower="-2.4" upper="2.4" effort="10" velocity="2"/>
  </joint>
  <link name="link_4">
    <inertial><mass value="0.08"/><inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/></inertial>
    <visual><geometry><box size="0.02 0.04 0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.02 0.04 0.02"/></geometry></collision>
  </link>
  <joint name="joint_4" type="revolute">
    <parent link="link_3"/><child link="link_4"/>
    <origin xyz="0 0.035 0" rpy="0 0 0"/><axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="8" velocity="2"/>
  </joint>
  <link name="link_5">
    <inertial><mass value="0.05"/><inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/></inertial>
    <visual><geometry><box size="0.018 0.03 0.018"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.018 0.03 0.018"/></geometry></collision>
  </link>
  <joint name="joint_5" type="revolute">
    <parent link="link_4"/><child link="link_5"/>
    <origin xyz="0 0.02 0" rpy="0 0 0"/><axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="5" velocity="2"/>
  </joint>
  <link name="link_6">
    <inertial><mass value="0.04"/><inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/></inertial>
    <visual><geometry><box size="0.015 0.02 0.015"/></geometry><material name="black"/></visual>
    <collision><geometry><box size="0.015 0.02 0.015"/></geometry></collision>
  </link>
  <joint name="joint_6" type="revolute">
    <parent link="link_5"/><child link="link_6"/>
    <origin xyz="0 0.015 0" rpy="0 0 0"/><axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="5" velocity="2"/>
  </joint>
  <link name="finger_left">
